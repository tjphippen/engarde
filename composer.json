{
    "name": "tjphippen/engarde",
    "description": "Engarde Web Scraper API for Laravel 5",
    "keywords": ["Engarde", "api", "scrape", "laravel", "laravel 5"],
    "homepage": "https://github.com/tjphippen/engarde",
    "license": "MIT",
    "authors": [
        {
            "name": "TJ Phippen",
            "email": "tj@tjphippen.com"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "illuminate/support": "~5.1",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Tjphippen\\Engarde\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tjphippen/engarde"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
